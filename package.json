{
  "name": "audiophile-ui",
  "version": "1.2.1",
  "private": true,
  "scripts": {
    "start": "next dev",
    "build": "next build",
    "serve": "next start",
    "lint": "eslint .",
    "test": "jest",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "test:coverage": "jest --coverage",
    "open:coverage": "open ./coverage/lcov-report/index.html",
    "test:coverage:open": "run-s test:coverage open:coverage",
    "cy:run": "cypress run --headless --browser chrome",
    "cy:open": "cypress open --browser chrome",
    "test:e2e": "is-ci test:e2e:ci test:e2e:local",
    "test:e2e:local": "start-server-and-test start 3000 cy:open",
    "test:e2e:ci": "start-server-and-test serve 3000 cy:run",
    "test:unit:ci": "start-server-and-test serve 3000 test",
    "pretest:unit:ci": "run-s clean build",
    "pretest:e2e:ci": "run-s clean build",
    "lint-staged": "lint-staged",
    "release": "release-it --ci",
    "prepare": "husky install",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint build",
    "clean": "rimraf .next"
  },
  "dependencies": {
    "@hookform/resolvers": "2.9.7",
    "@reduxjs/toolkit": "1.8.5",
    "@tanstack/react-query": "4.2.3",
    "next": "12.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.34.2",
    "react-redux": "8.0.2",
    "reset-css": "5.0.1",
    "sass": "1.54.8",
    "styled-components": "5.3.5",
    "styled-media-query": "2.1.2",
    "uuid": "9.0.0",
    "waait": "1.0.5",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@swc/core": "1.2.249",
    "@swc/jest": "0.2.22",
    "@tanstack/react-query-devtools": "4.2.3",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "18.7.14",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "5.1.26",
    "@types/uuid": "8.3.4",
    "auto-changelog": "2.4.0",
    "cypress": "10.7.0",
    "eslint": "8.23.0",
    "eslint-config-dva": "0.0.16",
    "eslint-plugin-cypress": "2.12.1",
    "husky": "8.0.1",
    "is-ci-cli": "2.2.0",
    "jest": "29.0.2",
    "jest-environment-jsdom": "29.0.2",
    "jest-watch-typeahead": "2.1.1",
    "lint-staged": "13.0.3",
    "local-cypress": "1.2.6",
    "msw": "0.47.0",
    "npm-run-all": "4.1.5",
    "release-it": "15.4.1",
    "rimraf": "3.0.2",
    "start-server-and-test": "1.14.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.2",
    "whatwg-fetch": "3.6.2"
  }
}
